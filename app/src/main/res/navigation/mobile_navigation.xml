    <fragment
        android:id="@+id/navigation_upnhac"
        android:name="com.project_karaoke.ui.upnhac.UpNhacFragment"
        android:label="@string/title_upnhac"
        tools:layout="@layout/fragment_up_nhac" />
    <fragment
        android:id="@+id/navigation_profile"
